<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>march_fake_sensor_data</name>
  <version>0.0.0</version>
  <description>Publishes fake sensor data for testing without hardware connected</description>
  <maintainer email="software@projectmarch.nl">Project MARCH</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>sensor_msgs</depend>
  <depend>march_shared_msgs</depend>
  <depend>march_utility</depend>

  <test_depend>ament_cmake_gtest</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
